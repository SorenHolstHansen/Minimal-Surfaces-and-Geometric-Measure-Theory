
\pagestyle{empty}

\setlength{\parskip}{0.5em}
\setlength{\parindent}{0em}

\makeatletter  % to avoid error messages generated by "\@". Makes Latex treat "@" like a letter

\linespread{1.0}
\def\submitdate#1{\gdef\@submitdate{#1}}
\def\degree#1{\gdef\@degree{#1}}
\def\supervisor#1{\gdef\@supervisor{#1}}

\def\maketitle{
    
  \begin{titlepage}{
    
    \centering{
    \includegraphics[width=0.5\columnwidth]{images/ku.jpg}} \par
    
    \vskip 1in \par 
    \LARGE {\bf \@title}
    

  }
  \vskip 0.3in \par
  \large {\bf \@author} \par
  
  \vskip 0.3in \par
  \large {\bf Supervised by \@supervisor}
  \vskip 0.3in \par
  \normalsize { July 2, 2020}
  
  \end{titlepage}
}

\def\titlepage{
  \newpage
  \centering
  \linespread{1}
  \normalsize
  \vbox to \vsize\bgroup\vbox to 9in\bgroup
}
\def\endtitlepage{
  \par
  \kern 0pt
  \egroup
  \vss
  \egroup
}

\def\abstract{
  \begin{center}{
    \large\bf Abstract}
  \end{center}
  \small
  %\def\baselinestretch{1.5}
  \linespread{1.5}
  \normalsize
}
\def\endabstract{
  \par
}


\def\preface{
    \pagenumbering{roman}
    \pagestyle{plain}
    \doublespacing
}

\def\body{
	
    \cleardoublepage   
    \pagestyle{fancy}
    \tableofcontents
    \pagestyle{plain}
    \cleardoublepage
    \pagestyle{fancy}
    \pagenumbering{arabic}
    \doublespacing
}

\makeatother  %to avoid error messages generated by "\@". Makes Latex treat "@" like a letter